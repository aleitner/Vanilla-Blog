<h1>Listing users</h1>

<table border="1" style="width:300px">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Handle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.full_name %></td>
        <td><%= user.email %></td>
        <td><%= user.user_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
